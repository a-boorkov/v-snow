{
  "compilerOptions": {
    "outDir": "./built/",
    "sourceMap": true,
    "strict": true,
    "noImplicitReturns": true,
    "module": "es2015",
    "moduleResolution": "node",
    "target": "es5",
    "strictPropertyInitialization": false,
    "experimentalDecorators": true,
    "noImplicitAny": false
  },
  "include": ["./src/**/*"]
}
